@model RegistrationCompletedViewModel
<form asp-controller="account">
    <div class="container-centered">
        <div class="all-elements">
            <img src="~/images/authentication/logo.png"
                 srcset="~/images/authentication/logo@2x.png 2x, ~/images/authentication/logo@3x.png 3x">
        </div>
        <div>
            <div class="form-group">
                <label class="sign-in-sign-up-label">@Model.Result</label>                
            </div>
            <div class="form-group">
                <button name="button" type="submit" value="login" class="sign-in-sign-up-active-button sign-in-sign-up-active-button-centered sign-in-sign-up-active-button-text">SIGN IN</button>
            </div>
        </div>
    </div>    
</form>

